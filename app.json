{
  "expo": {
    "name": "universal-app-basic",
    "slug": "universal-app-basic",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "unifi",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "icon": "./assets/images/icon-ios.png",
      "splash": {
        "image": "./assets/images/splash-logo-final.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      },
      "supportsTablet": true,
      "bundleIdentifier": "my.com.tm.uniapp",
      "appleTeamId": "CU86D2CJR3"
    },
    "android": {
      "icon": "./assets/images/icon-android.png",
      "splash": {
        "image": "./assets/images/splash-android.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "my.com.tm.uniapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "./plugins/with-face-module"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
